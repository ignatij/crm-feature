{
  "name": "crm-feature",
  "packageManager": "yarn@4.0.2",
  "private": true,
  "workspaces": [
    "app",
    "ui"
  ],
  "scripts": {
    "app:start": "yarn workspace app run start",
    "ui:build": "yarn workspace ui run build",
